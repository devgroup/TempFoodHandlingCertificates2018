@model TempFoodHandlingCertificates2014.ViewModels.vmPage3

@{
    ViewBag.Title = "Page 3. Temporary Food Stall Application";
}
<div class="myPageTitle">Registration Page 3 of 5</div>
@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
       
        @*<div class="form-group">
           
            <img src="~/Content/images/TemperatureControl.png" />
        </div>*@

            <div class="form-group">
               


                <div class="col-md-9">
                    @Html.EditorFor(model => model.TemperatureControl, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.TemperatureControl, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input name="submit" id="prev" type="submit" value="&laquo; Prev" class="btn btn-primary" />
                    <span>Page 3 of 5</span>
                    <input name="submit" id="next" type="submit" value="Next &raquo;" class="btn btn-primary" />
                </div>
            </div>
        </div>
}


@section Scripts {
   
    @Scripts.Render("~/bundles/jqueryval")
    
    
}
